extend layout
block content
    link(rel='stylesheet', href='/css/lihl.css')
    .content
        .umdieladder
            h1#titel Akitos Legion TD 2 Inhouse League
            table#ladder.ladder
                tr.header
                    th.table_header#th0(onclick="sortTable(0)") Rank
                    th.table_header#th1(onclick="sortTable(1)") Name
                    th.table_header#th2(onclick="sortTable(2)") Elo
                    th.table_header#th3(onclick="sortTable(3)") Games
                    th.table_header#th4(onclick="sortTable(4)") Wins
    script(src="/js/ladder_sort.js")
    script(src="/js/lihl.js")
